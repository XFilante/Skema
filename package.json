{
  "name": "@filante/skema",
  "description": "Skema is a type-safe client generator for Filante framework.",
  "version": "0.0.2",
  "engines": {
    "node": ">=20.6.0"
  },
  "type": "module",
  "files": [
    "build",
    "!build/bin",
    "!build/tests"
  ],
  "exports": {
    ".": "./build/index.js"
  },
  "homepage": "https://github.com/XFilante/Skema#readme",
  "repository": "https://github.com/XFilante/Skema.git",
  "scripts": {
    "clean": "del-cli build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "quick:test": "node --import=./tsnode.esm.js --enable-source-maps bin/test.ts",
    "test": "c8 nr quick:test",
    "prebuild": "nr clean",
    "build": "tsc && adonis-kit index build/commands",
    "changelog": "na changeset",
    "release:version": "changeset version",
    "release": "nr release:version && changeset publish",
    "version": "nr build",
    "prepublishOnly": "nr build",
    "check": "turbo format lint typecheck test",
    "postinstall": "husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "playground": "node --import=./tsnode.esm.js --enable-source-maps bin/playground.ts"
  },
  "devDependencies": {
    "@adonisjs/core": "^6.15.1",
    "@adonisjs/eslint-config": "^2.0.0-beta.7",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@changesets/cli": "^2.27.9",
    "@japa/assert": "^3.0.0",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.6.3",
    "@types/node": "^20.14.5",
    "c8": "^10.1.2",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "eslint": "^9",
    "husky": "^9.1.6",
    "pinst": "^3.0.0",
    "prettier": "^3.3.2",
    "ts-node-maintained": "^10.9.4",
    "turbo": "^2.3.3",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@vinejs/vine": "3.0.0",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3"
  },
  "author": "mohitxskull, XFilante",
  "license": "MIT",
  "keywords": [],
  "prettier": "@adonisjs/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "c8": {
    "exclude": [
      "tests/**"
    ]
  },
  "packageManager": "pnpm@9.14.2+sha512.6e2baf77d06b9362294152c851c4f278ede37ab1eba3a55fda317a4a17b209f4dbb973fb250a77abc463a341fcb1f17f17cfa24091c4eb319cda0d9b84278387",
  "dependencies": {
    "tinyexec": "^0.3.1",
    "ts-morph": "^24.0.0"
  }
}
